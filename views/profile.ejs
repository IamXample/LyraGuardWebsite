<body id="top">

  <!-- 
    - #HEADER
  -->

  <header class="header">

    <!-- 
      - overlay
    -->
    <div class="overlay" data-overlay></div>

    <div class="container">

      <a href="#" class="logo">
        <img src=" /images/logo.svg" alt="Supreme Homies">
      </a>

      <button class="nav-open-btn" data-nav-open-btn>
        <ion-icon name="menu-outline"></ion-icon>
      </button>

      <nav class="navbar" data-nav>

        <div class="navbar-top">
           <button class="btn-sign_in">


             <% if(is_logged) {%>
                 
              <span style="color: #FFFFFF;"><%= user.username %>#<%= user.discriminator %></span>
            <%} else {%>
    <img src="/images/logo.svg" ></img>
                <%}%>
        </button>
          <button class="nav-close-btn" data-nav-close-btn>
            <ion-icon name="close-outline"></ion-icon>
          </button>

        </div>

        <ul class="navbar-list">

          <li>
            <a href="/" class="navbar-link">Home</a>
          </li>

          <li>
            <a href="#about" class="navbar-link">About</a>
          </li>


          <li>
            <a href="#guilds" class="navbar-link">Guilds</a>
          </li>

          <li>
            <a href="/#gears" class="navbar-link">Shop</a>
          </li>

          <li>
            <a href="#contact" class="navbar-link">Contact</a>
          </li>

        

        </ul>

        <ul class="nav-social-list">

          <li>
            <a href="/twitter" class="social-link">
              <ion-icon name="logo-twitter"></ion-icon>
            </a>
          </li>

          <li>
            <a href="/instagram" class="social-link">
              <ion-icon name="logo-instagram"></ion-icon>
            </a>
          </li>

          <li>
            <a href="/github" class="social-link">
              <ion-icon name="logo-github"></ion-icon>
            </a>
          </li>

          <li>
            <a href="/youtube" class="social-link">
              <ion-icon name="logo-youtube"></ion-icon>
            </a>
          </li>

        </ul>

      </nav>

      <div class="header-actions">

        <button class="btn-sign_in">

          <div class="icon-box">
              <% if(is_logged) {%>
              <img src="https://cdn.discordapp.com/avatars/<%= user.id %>/<%= user.avatar %>.png?size=512%>" ></img>
            <%} else {%>
  <ion-icon name="log-in-outline"></ion-icon>
                <%}%>
            
          </div>

             <% if(is_logged) {%>
              <span><%= user.username %>#<%= user.discriminator %></span>
            <%} else {%>
  <span><a href="/auth/login" style="color: #FFFFFF;">Log-in</a></span>
                <%}%>
        </button>

      </div>

    </div>

  </header>





  <main>
    <article>

      <!-- 
        - #HERO
      -->

      




      <div class="section-wrapper">

        <!-- 
          - #ABOUT
        -->

        <section class="about" id="about">
          <div class="container">

            <figure class="about-banner">

              <img src="https://cdn.discordapp.com/avatars/<%= user.id %>/<%= user.avatar %>.png?size=512" alt="M shape" class="about-img">

            </figure>

            <div class="about-content">

<center>
              <h2 class="about-title"><%= user.username %><strong>#<%= user.discriminator %></strong> </h2> 

              <p class="about-text">
                <%= user.id %>
              </p>

              <p class="about-bottom-text">

                <% if(user.email) {%>
             <span><%= user.email %></span>
            <%} else {%>
  <span>No email</span>
                <%}%>
                  
              </p></center>
            

            </div>

          </div>
        </section>



        <!-- 
          - #TEAM
        -->

        <section class="team" id="guilds">
          <div class="container">

            <h2 class="h2 section-title">Your Guilds</h2>

            <ul class="team-list">
            <% guilds.forEach((guild) => { %>
              <div title="<%= guild.name %>">
                    <li>
             
                        <a href="/server/<%= guild.id %>">
                             <figure>
                <% if (guild.icon) { %>
                  <img src="https://cdn.discordapp.com/icons/<%= guild.id %>/<%= guild.icon %>.png?size=2048" width="70px" height="70px" class="team-member" />
       
              
                
                <% } else { %>
                  <canvas class="team-member" id="<%= guild.name.replace(/\w+/g, name => name[0]).replace(/\s/g, '') %>" width="70px" height="70px"></canvas>
                  <script>
                    function createIcone() {
                      const nameAcronym = document.getElementById("<%= guild.name.replace(/\w+/g, name => name[0]).replace(/\s/g, '')  %>").getContext('2d');
                    }
                    createIcone();
                  </script>
                <%}%>
                    </figure>
                </a>
                    
                             </li>
              </div>
            <%});%>
         

                
              

              

